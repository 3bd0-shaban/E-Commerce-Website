        <body id="body-pd">
            <!-- Side bar -->
                <%- include("./partials/sidebar") %> 
                <%- include("./partials/navbar") %>     


            


            <!-- Related Products box -->
            <div class="section container-fluid " id="Related">
                <p class="h3 d-flex justify-content-center py-3">Related Products</p>
                <div class="track ">
                    <button class="btn leftarrow d-none d-md-block"><i class='fas fa-angle-left' style='font-size:36px'></i></button>

                    <%  arrproduct.forEach(item => {  %>

                    <div class="card-container col-2 col-md-4 col-lg-2">
                      <div class="card ">
                        <a href="#" target="_blank" class="divimage"><img src="../Images/phone4.jpg" alt="Lights" class="relatedProductsimage"><div class="caption"></div></a>
                        <div class="namedes ml-4">
                            <h6 class="productname mt-5"><%= item.product_name %></h6>
                            <p class="productsmalldes"><%= item.product_name %></p>
                        </div>
                        <div class="row productboxdetail m-2 mt-4">
                            <div class="ratingProduct col">
                            <span class="bi bi-star checked"></span>
                            <span class="bi bi-star checked"></span>
                            <span class="bi bi-star checked"></span>
                            <span class="bi bi-star"></span>
                            <span class="bi bi-star"></span>
                            </div>
                            <h6 class="  ml-auto">$159</h6>
                        </div>
                            <div class="row btnonhoverproductbox d-flex justify-content-center">
                                <a href="/mainpage/<%= item._id %>" type ="sybmit"class="btn btnAddCartproduct whitelistbtn col-10 py-2 nowrap">Add to Card</a>
                            </div>
                      </div>
                    </div>
                        <%  }); %>
                      <button class="btn Rightarrow d-none d-md-block "><i class='fas fa-angle-left' style='font-size:36px'></i></button>
                </div>
            </div>




